#include<iosteam>
using namespace std;
int main{
    int n;
    cout<<"enter a number"<<endl;
    cin>>n;
    if(n==2){
          cout<<"prime number"<<endl;
          return 0;
    }
    int temp=0;
    for(int i=2;i<n;i++){
        if(n%i==0){
            temp=1;
        }
    }
    if(temp==0)
    cout<<"prime number"<<endl;
    if(temp==1){
    cout<<"not prime number"<<endl;
    }
}